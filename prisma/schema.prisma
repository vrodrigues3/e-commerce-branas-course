generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int    @id @unique
  description String
  price       Int
  width       Int
  height      Int
  length      Int
  weight      Float
}

model Coupon {
  code String @unique
  percentage Int
  expireDate DateTime
}
